  <script type="text/javascript">
    function validar_comentario() {
        // Campo a validar.
        campo = document.getElementById("comentario").value;
        // La condición.
        if (campo.length == 0) {
            alert("Por favor escribe tu comentario");
            document.getElementById("comentario").focus();
            return false;
        }

        // Campo a validar.
        campo = document.getElementById("comentario").value;
        // La condición.
        if (campo.length < 10) {
            alert("Ingresa un comentario de al menos 10 caracteres");
            document.getElementById("comentario").focus();
            return false;
        }
        return true;
    }
</script> 
