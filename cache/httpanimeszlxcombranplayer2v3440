            $('.versao').click(function () {
                $("#textoDownload").show();
                $("#textoDownload").html('Gerando link de download, aguarde...');
                var id = $(this).attr('id');
                url = versoes[id];
                $.ajax({
                    type: 'POST',
                    url: 'verificarDownloadAniteca.php',
                    data: 'url=' + url,
                    success: function (data) {
                        link = data.link;
                        $("#textoDownload").html('<a href="' + link + '" target="_blank">Clique aqui para fazer download.</a>');
