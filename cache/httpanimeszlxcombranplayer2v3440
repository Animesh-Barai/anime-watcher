            var playstation = 0;
            flowplayer(function (api) {
                api.on("load", function (e, api, video) {
                    var result = 'mq';
                    video.vodQualities.qualities.forEach(function (obj) {
                        if (obj.src === video.src) {
                            result = obj.label.toLowerCase();
                        }
                    });
                    if (WebTorrent.WEBRTC_SUPPORT && !playstation) {
                        if (typeof client !== 'undefined') {
                            client.destroy();
                        }
                        client = new WebTorrent();
                        client.add(magnets[result], function (torrent) {
                            // Stream the file in the browser
                            torrent.files[0].renderTo('#anplayer');
                        });
                    }
                });
            });
